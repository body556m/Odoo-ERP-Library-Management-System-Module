<odoo>
    <data>
        <record id="library_books_sequence" model="ir.sequence">
            <field name="name">library.books.sequence</field>
            <field name="code">library_books_seq</field>
            <field name="prefix">BOOK\</field>
            <field name="padding">4</field>
        </record>

        <record id="seq_library_member" model="ir.sequence">
            <field name="name">Library Member Sequence</field>
            <field name="code">library.member</field>
            <field name="prefix">MEM/</field>
            <field name="padding">5</field>
        </record>

        <record id="seq_library_reservation" model="ir.sequence">
            <field name="name">Library Reservation Sequence</field>
            <field name="code">library.reservation.sequence</field>
            <field name="prefix">RES/</field>
            <field name="padding">5</field>
        </record>
    </data>
</odoo>
